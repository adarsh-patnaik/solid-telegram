class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int cnt = 0;

        for (int i = 0; i < nums.size(); i++) {
            int digits = 0;
            int n = nums[i];
            while (n > 0) {
                n /= 10;
                digits++;
            }
            if (digits % 2 == 0) {
                cnt++;
            }
        }
        return cnt;
    }
};
